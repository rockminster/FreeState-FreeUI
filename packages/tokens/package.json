    "clean": "rm -rf dist"
  },
  "keywords": [
    "design-tokens",
    "design-system",
    "tokens"
  ],
  "author": "rockminster",
  "license": "MIT",
  "devDependencies": {
    "@freeui/eslint-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}